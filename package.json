{
  "name": "@bumble/chrome-rxjs",
  "version": "0.10.1",
  "description": "RxJS Observables for Chrome API events.",
  "license": "MIT",
  "author": "Jack and Amy Steam <jacksteamdev@gmail.com>",
  "files": [
    "lib",
    "types"
  ],
  "main": "lib/index-cjs.js",
  "module": "lib/index-esm.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "run-p build:dev",
    "build:dev": "rollup -c --environment NODE_ENV:development",
    "build:pro": "rollup -c --environment NODE_ENV:production",
    "build:types": "tsc -p tsconfig.d.json",
    "build:watch": "npm run build:dev -- -w",
    "release": "git push && npm publish",
    "start": "run-p build:watch",
    "test": "jest",
    "test:all": "jest",
    "test:e2e": "jest e2e",
    "test:quick": "jest --testPathIgnorePatterns=e2e",
    "postversion": "run-s build:pro build:types release"
  },
  "devDependencies": {
    "@types/chrome": "0.0.91",
    "@types/jest": "^24.0.19",
    "@types/node": "^12.11.7",
    "@types/power-assert": "^1.5.0",
    "@types/puppeteer": "^1.20.2",
    "@types/sinon-chrome": "^2.2.6",
    "chrome-promise": "^3.0.5",
    "delay": "^4.3.0",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "power-assert": "^1.6.1",
    "puppeteer": "^2.0.0",
    "rollup": "^1.25.2",
    "rollup-plugin-chrome-extension": "^1.2.6",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-empty-dir": "^1.0.4",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-zip": "^0.5.2",
    "sinon-chrome": "^3.0.1",
    "ts-jest": "^24.1.0",
    "tslib": "^1.10.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "rxjs": "^6.5.3"
  }
}
